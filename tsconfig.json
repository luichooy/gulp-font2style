{
  "compilerOptions": {
    "target": "ES5",
    "module": "CommonJS",
    "outDir": "./lib",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "esModuleInterop": true,
    "declaration": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}
